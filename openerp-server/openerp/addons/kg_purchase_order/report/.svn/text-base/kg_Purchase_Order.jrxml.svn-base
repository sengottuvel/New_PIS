	<subDataset name="dataset1">
		<queryString>
			<![CDATA[select
partner.name as company_name,

(select sum(purchase_order.amount_total) from purchase_order where purchase_order.partner_id = po.partner_id and purchase_order.state='approved') as po_value,
(select sum(kg_general_grn.amount_total) from kg_general_grn where kg_general_grn.supplier_id = po.partner_id ) as grn_value,
(select sum(kg_purchase_invoice.net_amt) from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id ) as invoice_value,
(select sum(kg_purchase_invoice.advance_adjusted_amt) from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id ) as advance_value,
(select sum(kg_purchase_invoice.bal_amt) from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id ) as balance_value
from purchase_order po
left join res_partner partner on (partner.id= po.partner_id)


where po.id = 450]]>
		</queryString>
		<field name="company_name" class="java.lang.String"/>
		<field name="po_value" class="java.math.BigDecimal"/>
		<field name="grn_value" class="java.math.BigDecimal"/>
		<field name="invoice_value" class="java.lang.Double"/>
		<field name="advance_value" class="java.lang.Double"/>
		<field name="balance_value" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="New Dataset 1">
		<parameter name="order_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select

pro_temp.name ||
(CASE 	WHEN brand.name!=''
THEN  ' | ' || brand.name
ELSE ''
END ) as product_name,
pro_uom.name as product_uom,
po_line.product_qty as po_qty,
po_line.product_id as pro_id,
po_line.date_planned as po_date,
trim(TO_CHAR(po_line.price_unit::float, '999G999G99G999G99G99G990D99'))  as po_rate,
trim(TO_CHAR((po_line.product_qty * po_line.price_unit)::float, '999G999G99G999G99G99G990D99'))  as value,
po_line.kg_discount_per as disc_per,
(select TO_CHAR(MAX(round(price_unit,2)),'999G999G990D00') AS price from purchase_order_line where po_line.product_id=purchase_order_line.product_id and purchase_order_line.date_planned::date <=po_line.date_planned) as max_price,
(select TO_CHAR(MIN(round(price_unit,2)), '999G999G990D00') AS price from purchase_order_line where purchase_order_line.product_id=po_line.product_id and purchase_order_line.date_planned::date <=po_line.date_planned) as min_price,
(select TO_CHAR(round(price_unit,2), '999G999G990D00') AS price from purchase_order_line where purchase_order_line.product_id=po_line.product_id and purchase_order_line.date_planned::date <=po_line.date_planned order by date_planned DESC limit 1) as rec_price,

case when po_line.id is not null then
(SELECT CAST(string_agg(description, ',') AS text)
FROM account_tax where id in (select tax_id from purchase_order_taxe where ord_id=po_line.id))
end as tax_name

from purchase_order_line po_line


left join product_template pro_temp on (pro_temp.id = po_line.product_id)

left join product_uom pro_uom on (pro_uom.id = po_line.product_uom)

left join kg_brand_master brand on (brand.id = po_line.brand_id)


where po_line.order_id = $P{order_id} order by po_line.id]]>
		</queryString>
		<field name="product_name" class="java.lang.String"/>
		<field name="product_uom" class="java.lang.String"/>
		<field name="po_qty" class="java.lang.Double"/>
		<field name="pro_id" class="java.lang.Integer"/>
		<field name="po_date" class="java.sql.Date"/>
		<field name="po_rate" class="java.lang.String"/>
		<field name="value" class="java.lang.String"/>
		<field name="disc_per" class="java.math.BigDecimal"/>
		<field name="max_price" class="java.lang.String"/>
		<field name="min_price" class="java.lang.String"/>
		<field name="rec_price" class="java.lang.String"/>
		<field name="tax_name" class="java.lang.String"/>
	</subDataset>
	<subDataset name="dataset2"/>
	<parameter name="po_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select po.id as id,
comp.name as company_name,
part.street as street1,
part.street2 as street2,
comp_city.name as city,
comp_state.name as state,
comp_country.name as country,
part.zip as zip,
part.phone as phone,
part.fax as fax,
part.email as email,

CASE WHEN comp.cin_no != '' THEN 'CIN NO :'
ELSE ''
END as cin_label,
CASE WHEN comp.tin_no != '' THEN 'TIN NO :'
ELSE ''
END as tin_label,
comp.cin_no as cin_no,
comp.tin_no as tin_no,
part.website as website,

po.name as po_name,
to_char(po.date_order::date,'dd-mm-YYYY') as po_date,

partner.name as supplier_name,
partner.street as sup_street1,
partner.street2 as sup_street2,
partner.contact_person as contact_person,
partner.phone as sup_phone,
partner.fax as sup_fax,
partner.email as sup_email,
CASE WHEN partner_city.name = null THEN ''
ELSE partner_city.name
END as sup_city,
CASE WHEN partner_state.name = null THEN ''
ELSE partner_state.name
END as sup_state,
CASE WHEN partner_country.name = null THEN ''
ELSE partner_country.name
END as sup_country,
CASE WHEN partner.zip = null THEN ''
ELSE partner.zip
END as sup_zip,
CASE WHEN partner.mobile = null THEN ''
ELSE partner.mobile
END as sup_mobile,

po.quot_ref_no as quot_ref_no,
po.dep_project_name as project_name,
trim(TO_CHAR(po.amount_untaxed::float, '999G999G99G999G99G99G990D99'))  as total_amt,
trim(TO_CHAR(po.discount::float, '999G999G99G999G99G99G990D99'))  as discount,

po.amount_tax as tax,
trim(TO_CHAR(po.amount_total::float, '999G999G99G999G99G99G990D99'))   as po_amt,
trim(TO_CHAR((po.value1 + po.value2)::float, '999G999G99G999G99G99G990D99'))  as other_charge,

payment.name as payment,
delivery.name as delivery,
po.term_freight as frieght,
CASE WHEN po.bill_type='cash' THEN 'Cash'
ELSE 'Credit'
END as bill,
po.term_warranty as warranty,
po.version as version,
CASE WHEN po.term_price='inclusive' THEN 'Inclusive of all taxes and duties'
ELSE '-'
END as price,
f_amount_to_words(po.amount_total,po.company_id) as amount_in_words,
CASE WHEN comp.rml_header1 = null THEN null
ELSE comp.rml_header1
END as tagline,
po.note as specification,
po.state as status,
(CASE 	WHEN (select TO_CHAR(round(sum(purchase_order.amount_total)), '99G99G990D99') from purchase_order where purchase_order.partner_id = po.partner_id and purchase_order.state='approved' ) IS NOT NULL
THEN   (select TO_CHAR(round(sum(purchase_order.amount_total)), '99G99G990D99') from purchase_order where purchase_order.partner_id = po.partner_id and purchase_order.state='approved' )
ELSE '0'
END ) as po_value,
(CASE 	WHEN (select TO_CHAR(round(sum(kg_general_grn.amount_total)+(select sum(kg_po_grn.amount_total)from kg_po_grn where kg_po_grn.supplier_id = po.partner_id and kg_po_grn.state='inv'),2), '99G99G990D99') from kg_general_grn where kg_general_grn.supplier_id = po.partner_id and kg_general_grn.state='inv'  ) IS NOT NULL
THEN   (select TO_CHAR(round(sum(kg_general_grn.amount_total)+(select sum(kg_po_grn.amount_total)from kg_po_grn where kg_po_grn.supplier_id = po.partner_id and kg_po_grn.state='inv'),2), '99G99G990D99') from kg_general_grn where kg_general_grn.supplier_id = po.partner_id and kg_general_grn.state='inv')
ELSE '0.00'
END ) as grn_value,
(CASE 	WHEN (select TO_CHAR((round(sum(kg_po_grn.amount_total))), '99G99G990D99') from kg_po_grn where kg_po_grn.supplier_id = po.partner_id and kg_po_grn.state='inv'  ) IS NOT NULL
THEN   (select TO_CHAR(round(sum(kg_po_grn.amount_total)), '99G99G990D99') from kg_po_grn where kg_po_grn.supplier_id = po.partner_id and kg_po_grn.state='inv')
ELSE '0.00'
END ) as grn_po_value,
(CASE 	WHEN (select TO_CHAR(round(sum(kg_purchase_invoice.net_amt)), '99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved') IS NOT NULL
THEN   (select TO_CHAR(round(sum(kg_purchase_invoice.net_amt)), '99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved' )
ELSE '0.00'
END ) as invoice_value,

(CASE 	WHEN (select TO_CHAR(round(sum(kg_purchase_invoice.advance_adjusted_amt)), '99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved') IS NOT NULL
THEN   (select TO_CHAR(round(sum(kg_purchase_invoice.advance_adjusted_amt)), '99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved' )
ELSE '0.00'
END ) as advance_value,
(CASE 	WHEN (select TO_CHAR(round(sum(kg_purchase_invoice.bal_amt)), '99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved') IS NOT NULL
THEN   (select TO_CHAR(round(sum(kg_purchase_invoice.bal_amt)), '99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved' )
ELSE '0.00'
END ) as balance_value,



(CASE 	WHEN (select TO_CHAR(round(sum(kg_purchase_invoice.net_amt))-round(sum(kg_purchase_invoice.bal_amt)),'99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved') IS NOT NULL
THEN   (select TO_CHAR(round(sum(kg_purchase_invoice.net_amt))-round(sum(kg_purchase_invoice.bal_amt)),'99G99G990D99') from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved' )
ELSE '0.00'
END ) as paid_against_inv,

(CASE 	WHEN (select round(sum(kg_purchase_invoice.bal_amt)) from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved') IS NOT NULL
THEN   (select round(sum(kg_purchase_invoice.bal_amt)) from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved' )
ELSE '0.00'
END ) as balance_cal,

(CASE 	WHEN (select round(sum(kg_purchase_invoice.net_amt)) from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved') IS NOT NULL
THEN   (select round(sum(kg_purchase_invoice.net_amt)) from kg_purchase_invoice where kg_purchase_invoice.supplier_id = po.partner_id and kg_purchase_invoice.state='approved' )
ELSE '0.00'
END ) as invoice_cal





from purchase_order po

left join res_company comp on (comp.id = po.company_id)

left join res_partner part on (part.id = comp.partner_id)

left join res_partner partner on (partner.id = po.partner_id)

left join res_city comp_city on (comp_city.id = comp.city)

left join res_city partner_city on (partner_city.id = partner.city)

left join res_country_state comp_state on (comp_state.id = comp.state_id)

left join res_country_state partner_state on (partner_state.id = partner.state_id)

left join res_country comp_country on (comp_country.id = comp.country_id)

left join res_country partner_country on (partner_country.id = partner.country_id)

left join kg_payment_master payment on (payment.id = po.payment_mode)

left join kg_delivery_master delivery on (delivery.id = po.delivery_mode)

left join kg_purchase_invoice inv on (inv.supplier_id = po.partner_id and inv.state ='approved')

where po.id = $P{po_id}]]>
	</queryString>
